<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="600"
        x:Class="DevTestToolsByAvalonia.JsonResultWindow"
		xmlns:devtesttoolsbyavalonia="using:DevTestToolsByAvalonia"
        Title="JsonResultWindow"
		TransparencyLevelHint="AcrylicBlur"
	Width="800" Height="600">
	
	<Grid ShowGridLines="False" Margin="0"
      ColumnDefinitions="325,150,325 "
      RowDefinitions="auto, auto,auto">
		<Label Grid.Row="0" Grid.Column="0" Margin="40 10" HorizontalAlignment="Center">需处理的Json </Label>
		<Label Grid.Row="0" Grid.Column="1" Margin="40 10" HorizontalAlignment="Center">操作区 </Label>
		<Label Grid.Row="0" Grid.Column="2" Margin="40 10" HorizontalAlignment="Center"> 条件、饲料、证据</Label>
		<TextBox Name="JsonTextOutPut"  AcceptsReturn="True" Grid.Row="1" Grid.Column="0" Width="300" Height="500" Margin="0 5"  />
		<Button Grid.Row="1" Grid.Column="1" Margin="10 10" Click="Creat_Json_btn_C"  HorizontalAlignment="Center">Creat Comment</Button>
		<TextBox Name="JsonTextInPut"  AcceptsReturn="True" Grid.Row="1" Grid.Column="2" Width="300" Height="500"  Margin="0 5"  />
	</Grid>

</Window>
